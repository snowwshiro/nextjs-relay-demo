  },
  "devDependencies": {
    "@types/react": "17.0.18",
    "@types/react-relay": "^11.0.2",
    "babel-plugin-relay": "^11.0.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "graphql": "^15.5.1",
    "relay-compiler": "^11.0.2",
    "typescript": "4.3.5"
  }
}
